<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="sidebar.main">
            <block class="Magento\Cms\Block\Block" name="Imagen-debajo-filtros" after="catalog.leftnav">
                <arguments>
                    <argument name="block_id" xsi:type="string">Imagen-debajo-filtros</argument>
                </arguments>
            </block>
        </referenceContainer>

        <move element="breadcrumbs" destination="page.wrapper" before="main.content" />
    </body>
</page>
