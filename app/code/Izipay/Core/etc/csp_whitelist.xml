<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
         <policy id="script-src">
            <values>
                <value id="izipay.pe" type="host">*.izipay.pe</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="connect-izipay-pe" type="host">*.izipay.pe</value>
            </values>
        </policy>
         <policy id="frame-src">
            <values>
                <value id="frame-izipay-pe" type="host">*.izipay.pe</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>