<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <referenceBlock name="page.title">
        <action method="setPageTitle">
            <argument name="title" xsi:type="string">Log Izipay</argument>
        </action>
    </referenceBlock>
    <body>
        <referenceContainer name="content">
            <block class="Izipay\Core\Block\Adminhtml\Detail" name="izipay.log.block.adminhtml.detail" template="Izipay_Core::log_detail.phtml"/>
        </referenceContainer>
    </body>
</page>