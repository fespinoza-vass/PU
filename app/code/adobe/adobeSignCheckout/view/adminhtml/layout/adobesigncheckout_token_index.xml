<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <referenceBlock name="page.title">
        <action method="setPageTitle">
            <argument name="title" xsi:type="string">Adobe Sign Refresh Token Helper</argument>
        </action>
    </referenceBlock>
    <body>
        <referenceContainer name="content">
            <block class="adobe\adobeSignCheckout\Block\RefreshToken" name="refreshToken" template="adobe_adobeSignCheckout::token.phtml"/>
        </referenceContainer>
    </body>
</page>
