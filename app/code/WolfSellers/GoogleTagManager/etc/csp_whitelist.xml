                <value id="hsforms-img" type="host">*.hsforms.com</value>
                <value id="facebook-img" type="host">*.facebook.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="analytics_tiktok_com" type="host">analytics.tiktok.com</value>
                <value id="forms_hscollectedforms_net" type="host">forms.hscollectedforms.net</value>
                <value id="doubleclick-connect" type="host">*.doubleclick.net</value>
