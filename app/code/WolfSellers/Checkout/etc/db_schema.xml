<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="sales_order">
        <column name="ubigeo_estimated_delivery" nullable="true" xsi:type="text" comment="Estimacioon de Entrega URBANO"/>
    </table>
    <table name="quote">
        <column name="customer_apellido" nullable="true" xsi:type="varchar" comment="customer lastname guest" length="200"/>
        <column name="customer_password" nullable="true" xsi:type="varchar" comment="customer lastname guest" length="200"/>
    </table>
</schema>

