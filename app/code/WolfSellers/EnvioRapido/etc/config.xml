                <sandbox_ws_url>https://test.savarexpress.com.pe/govari/api/</sandbox_ws_url>
                <production_order_endpoint>ServicioDelivery</production_order_endpoint>
                <sandbox_order_endpoint>ServicioDelivery</sandbox_order_endpoint>
