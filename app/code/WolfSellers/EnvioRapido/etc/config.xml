<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
		<carriers>
			<envio_rapido>
				<model>WolfSellers\EnvioRapido\Model\Carrier\EnvioRapido</model>
				<active>0</active>

                <sanbox_mode>1</sanbox_mode>

				<title>Envio Rapido</title>
